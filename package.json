  "description": "Copy and save your favorite snippets!",
  "main": "app/index.js",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish"
  },
  "keywords": [],
  "author": "nirkaufman",
  "license": "MIT",
  "config": {
    "forge": {
